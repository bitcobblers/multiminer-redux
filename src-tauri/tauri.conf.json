{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "multiminer-redux",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "open": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": false,
        "open": true,
        "save": true
      },
      "fs": {
        "all": false,
        "copyFile": false,
        "createDir": true,
        "exists": true,
        "readDir": true,
        "readFile": true,
        "removeDir": true,
        "removeFile": true,
        "renameFile": true,
        "scope": [
          "$DOWNLOAD",
          "$DOWNLOAD/*",
          "$APPLOCALDATA",
          "$APPLOCALDATA/**"
        ],
        "writeFile": true
      },
      "http": {
        "all": false,
        "request": true,
        "scope": [
          "https://api.github.com/repos/*",
          "https://github.com/*",
          "https://api.coingecko.com/api/v3/*",
          "http://localhost:60090/*"
        ]
      },
      "path": {
        "all": true
      }
    },
    "windows": [
      {
        "title": "multiminer-redux",
        "width": 1024,
        "height": 768
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "multiminer.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
